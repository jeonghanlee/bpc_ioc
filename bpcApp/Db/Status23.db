record(aSub, "$(P)Status-Sub"){
    field(SNAM, "StatSub")
    field(SCAN, ".23 second")

    field(FTA,  "STRING")
    field(NOA,  "20")
    field(INPA, "$(P)IP:Settings-SP")

    field(FTVA,  "FLOAT")
    field(NOVA,  "60")
    field(OUTA, "$(P)Status-Wfm PP")
    field(FTVB,  "LONG")
    field(NOVB,  "1")
    field(OUTB, "$(P)Heatsink1:PWM-I PP")
    field(FTVC,  "LONG")
    field(NOVC,  "1")
    field(OUTC, "$(P)Heatsink2:PWM-I PP")
    field(FTVD,  "LONG")
    field(NOVD,  "1")
    field(OUTD, "$(P)Heatsink3:PWM-I PP")
    field(FTVE,  "LONG")
    field(NOVE,  "1")
    field(OUTE, "$(P)Delta:PSMod:Status-I PP")
    field(FTVF,  "LONG")
    field(NOVF,  "1")
    field(OUTF, "$(P)Fault:Status-I PP")
    field(FTVG,  "LONG")
    field(NOVG,  "1")
    field(OUTG, "$(P)Serial:Number-I PP")
    field(FTVH,  "FLOAT")
    field(NOVH,  "1")
    field(OUTH, "$(P)Firmware:Version-I PP")
    field(FTVI,  "FLOAT")
    field(NOVI,  "1")
    field(OUTI, "$(P)Loop:Rate-I PP")
    field(FTVJ,  "LONG")
    field(NOVJ,  "1")
    field(OUTJ, "$(P)UDPpkt:Counter-I PP")
    field(FTVK,  "LONG")
    field(NOVK,  "1")
    field(OUTK, "$(P)Uptime-I PP")
    field(FTVL,  "LONG")
    field(NOVL,  "1")
    field(OUTL, "$(P)Frame:End-I PP")
    field(FTVM,  "LONG")
    field(NOVM,  "1")
    field(OUTM, "$(P)UDP:Status-I PP")

}

